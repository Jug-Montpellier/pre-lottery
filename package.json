{
  "name": "pre-lottery",
  "version": "1.0.0",
  "description": "Lottery of lottery for montpellier-jug",
  "main": "index.js",
  "scripts": {
    "dev": "npm run dev:init && npm run dev:assets && npm run dev:rollup",
    "dev:init": "mkdir -p dist",
    "dev:assets": "cp src/index.html dist && cp src/index.css dist && cp src/min.logo.svg dist",
    "dev:rollup": "rollup --watch -i src/index.js -o dist/bundle.js -f iife -m inline",
    "build": "npm run dev:init && rollup -i src/index.js -o dist/bundle.js -f iife",
    "deploy": "gh-pages -d .",
    "test": "echo \"Open tests/tests.html in your browser!\""
  },
  "author": "Romain Maneschi",
  "license": "MIT",
  "repository": {
    "type" : "git"
  , "url" : "git@github.com:manland/pre-lottery.git"
  },
  "dependencies": {
    "jasmine-core": "=2.5.2"
  },
  "devDependencies": {
    "gh-pages": "=0.11.0",
    "rollup": "=0.36.3",
    "rollup-watch": "=2.5.0"
  }
}
